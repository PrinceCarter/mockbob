{

    "manifest_version": 2,
    "name": "Mockbob",
    "version": "1.0",
  
    "description": "Adds a browser action icon to the toolbar. Click the button to replace all sentences that contain the text NYU with mockbob text.",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
    "icons": {
      "48": "icons/mockbob.jpg"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/mockbob.png",
      "default_title": "Mockbob",
      "default_popup": "popup/mocker.html"
    }
  }